#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/jayson/bin/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/jayson/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/jayson/bin/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules/jayson/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/jayson@4.2.0_bufferutil@4.0.9_utf-8-validate@5.0.10/node_modules:/Users/dylanboudro/Desktop/sundial.exchange/tools/referral-setup/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jayson/bin/jayson.js" "$@"
else
  exec node  "$basedir/../jayson/bin/jayson.js" "$@"
fi
